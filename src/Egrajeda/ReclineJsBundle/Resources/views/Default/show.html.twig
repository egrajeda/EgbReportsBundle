<!DOCTYPE html>
<html>
  <head>
    <title>Hello</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    {% stylesheets '@EgrajedaReclineJsBundle/Resources/public/css/*' filter='cssrewrite' %}
      <link href="{{ asset_url }}" rel="stylesheet">
    {% endstylesheets %}
  </head>
  <body>
    <div class="container">
      <div class="row">
        <div class="span12">
          <h1>Reportes</h1>
        </div>
      </div>
      <div class="row">
        <div class="span4">
          <div class="accordion">
            <div class="accordion-group">
              <div class="accordion-heading">
                <a class="accordion-toggle" data-toggle="collapse" href="#listado">
                  Listado de reportes
                </a>
              </div>
              <div id="listado" class="accordion-body collapse in">
                <div class="accordion-inner">
                  &nbsp;
                </div>
              </div>
            </div>
            <div class="accordion-group">
              <div class="accordion-heading">
                <a class="accordion-toggle" data-toggle="collapse" href="#filtros">
                  Filtros
                </a>
              </div>
              <div id="filtros" class="accordion-body collapse in">
                <div class="accordion-inner">
                  &nbsp;
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="span8">
          <div id="mygrid" style="height: 500px;"></div>
        </div>
      </div>
    </div>
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/underscore.js/1.4.4/underscore-min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/backbone.js/1.0.0/backbone-min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/mustache.js/0.7.2/mustache.min.js"></script>
    {% javascripts '@EgrajedaReclineJsBundle/Resources/public/js/*' %}
      <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
  </body>
</html>
